{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "DocumentFragment": true,
        "Parse": true,
        "rivets": true,
        "createjs": true,
        "alert": true,
        "Media": true,
        "cordova": true
    },
    "rules": {
        "valid-jsdoc": 0,
        "brace-style": [1, "stroustrup"],
        "consistent-this": [0, "self"],
        "no-constant-condition": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": 1,
        "func-names": 0,
        "new-cap": 1,
        "new-parens": 2,
        "no-ternary": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "quotes": [2, "single"],
        "one-var": 0,
        "space-infix-ops": 0,
        "strict": 0,
        "no-alert": 0
    }
}
